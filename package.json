{
    "name": "guyssembler",
    "displayName": "guyssembler",
    "description": "Language support for guyssembly",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.61.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "guy",
            "aliases": ["guy", "guy"],
            "extensions": ["guy"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "guy",
            "scopeName": "source.guy",
            "path": "./syntaxes/guy.tmLanguage.json"
        }]
    }
}
